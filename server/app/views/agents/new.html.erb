<%=form_for @agent do |f| %>
  <%=f.label :username %><br/>
  <%=f.text_field :username %>
  <br/>
  <%=f.label :agent_type %><br/>
  <%=f.text_field :agent_type %>
  <br/>
  <%=f.label :cookie %><br/>
  <%=f.text_area :cookie  %>
  <br/>
  <%=f.submit "Create agent", class: ["btn", "btn-primary"] %>
<% end %>
